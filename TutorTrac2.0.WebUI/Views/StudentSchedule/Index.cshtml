@model IEnumerable<TutorTrac2.core.Models.StudentSchedule>

@{
    ViewBag.Title = "Index";
}

<h2>My Class Schedule</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.c_code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.professor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.day)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.room)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.c_code)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.professor)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.day)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.time)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.room)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
