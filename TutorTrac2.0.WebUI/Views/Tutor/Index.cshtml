@model IEnumerable<TutorTrac2.core.Models.Tutors>

@{
    ViewBag.Title = "Index";
}

<h2>Tutors</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tut_f_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tut_l_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tut_email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tut_f_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tut_l_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tut_email)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Id }) 
        </td>
    </tr>
}

</table>
